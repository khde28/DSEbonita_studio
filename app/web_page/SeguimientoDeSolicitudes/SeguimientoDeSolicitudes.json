    "agregarEstatusSolicitudVacaciones" : {
      "type" : "expression",
      "value" : [
        "if($data.hasOwnProperty('solicitudVacaciones') && $data.solicitudVacaciones) {\r",
        "  for (let solicitud of $data.solicitudVacaciones) {\r",
        "    if(solicitud.esAprobada)  {\r",
        "      demande.esAprobadaEtiqueta = \"Aprobada\";\r",
        "    } else if(solicitud.esAprobada === false) {\r",
        "      demande.esAprobadaEtiqueta = \"Rechazada\";\r",
        "    } else {\r",
        "      demande.esAprobadaEtiqueta = \"En curso\";\r",
        "    }\r",
        "  }\r",
        "}\r",
        "\r",
        "return $data.solicitudVacaciones;"
